{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "mmcadocker/multi-client",
      "hostname": "client",
      "essential": false,
      "memory": 256
    },
    {
      "name": "nginx",
      "image": "mmcadocker/multi-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "memory": 256
    }
  ]
}


